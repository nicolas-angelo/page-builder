{
	"tabWidth": 2,
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"printWidth": 80,
	"endOfLine": "lf",
	"insertPragma": false,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"semi": true,
	"trailingComma": "es5",
	"useTabs": true,
	"singleQuote": true,
	"bracketSameLine": false,
  "plugins": ["prettier-plugin-tailwindcss"]
}
